{
  "name": "sanskrit-vscode",
  "displayName": "sanskrit-vscode",
  "description": "sanskrit-lang vscode plugin",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sanskrit-lang",
      "aliases": ["sanskrit-lang", "sanskrit-lang"],
      "extensions": [".scala",".sc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sanskrit-lang",
      "scopeName": "source.scala",
      "path": "./syntaxes/sanskrit-lang.tmLanguage.json"
    }]
  }
}
